     "text": [
      "usage: worldcat [-h] [-t TITLE] [-a AUTHOR] [-l LIMIT] [-o ORDER] [--output]\r\n",
      "                [--api] [--format]\r\n",
      "\r\n",
      "worldcat 0.1(Vikas Kundu) - description: Search books on Worldcat using author\r\n",
      "name or book title.\r\n",
      "\r\n",
      "optional arguments:\r\n",
      "  -h, --help            show this help message and exit\r\n",
      "  -t TITLE, --title TITLE\r\n",
      "                        Query string for title of a book\r\n",
      "  -a AUTHOR, --author AUTHOR\r\n",
      "                        Query string for author of a book\r\n",
      "  -l LIMIT, --limit LIMIT\r\n",
      "                        Search limit(number of results, default=25)\r\n",
      "  -o ORDER, --order ORDER\r\n",
      "                        Search order(asc or desc, default=desc)\r\n",
      "  --output, --output    Save the output to the workspace\r\n",
      "  --api, --api          Show results in the JSON format\r\n",
      "  --format, --format    Beautifying JSON output if --api is used\r\n"
     ]
    }
   ],
   "source": [
    "!maryam -e worldcat --help"
   ]
